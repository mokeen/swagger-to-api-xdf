{
  "name": "va-swagger-to-api",
  "publisher": "xdf-va",
  "displayName": "Va Swagger to API (Vue2/Vue3)",
  "description": "Generate TypeScript API client code from Swagger/OpenAPI v2 documentation with intelligent type system and incremental updates support. Works with Vue 2, Vue 3, and any TypeScript + Axios project.",
  "version": "2.0.5",
  "author": "xdf-va",
  "license": "ICT",
  "icon": "resources/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mokeen/swagger-to-api-xdf.git"
  },
  "homepage": "https://github.com/mokeen/swagger-to-api-xdf/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/mokeen/swagger-to-api-xdf/issues"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:swagger-to-api.addSwagger"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swagger-to-api.openAddSwagger",
        "title": "添加Swagger文档",
        "icon": {
          "light": "resources/light/add-doc-icon.svg",
          "dark": "resources/dark/add-doc-icon.svg"
        }
      },
      {
        "command": "swagger-to-api.refresh",
        "title": "Refresh Swagger List",
        "category": "Swagger to API"
      },
      {
        "command": "swagger-to-api.refreshList",
        "title": "刷新文档列表",
        "icon": {
          "light": "resources/light/refresh-doc-icon.svg",
          "dark": "resources/dark/refresh-doc-icon.svg"
        }
      },
      {
        "command": "swagger-to-api.deleteDoc",
        "title": "删除Swagger文档",
        "icon": {
          "light": "resources/light/delete-doc-icon.svg",
          "dark": "resources/dark/delete-doc-icon.svg"
        }
      },
      {
        "command": "swagger-to-api.previewDoc",
        "title": "预览Swagger文档",
        "icon": {
          "light": "resources/light/preview-doc-icon.svg",
          "dark": "resources/dark/preview-doc-icon.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "swagger-to-api.refreshList",
          "group": "navigation",
          "when": "view == swaggerExplorer"
        },
        {
          "command": "swagger-to-api.openAddSwagger",
          "group": "navigation",
          "when": "view == swaggerExplorer"
        }
      ],
      "view/item/context": [
        {
          "command": "swagger-to-api.deleteDoc",
          "when": "view == swaggerExplorer && viewItem == swaggerDoc",
          "group": "inline",
          "icon": "$(trashcan)"
        },
        {
          "command": "swagger-to-api.previewDoc",
          "when": "view == swaggerExplorer && viewItem == swaggerDoc",
          "group": "inline",
          "icon": "$(eye)"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger-to-api",
          "title": "Swagger To Api",
          "icon": "resources/menu-icon.svg"
        }
      ]
    },
    "views": {
      "swagger-to-api": [
        {
          "id": "swaggerExplorer",
          "name": "Swagger Docs"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "12.20.55",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "4.9.5"
  },
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  }
}
